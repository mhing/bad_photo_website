<% provide(:title, 'LA') %>
<div id="laCarousel" class="carousel" data-ride="carousel">
	<!-- indicators -->
	<ol class="carousel-indicators">
		<li data-target="#laCarousel" data-slide-to="0" class="active"></li>
		<li data-target="#laCarousel" data-slide-to="1"></li>
		<li data-target="#laCarousel" data-slide-to="2"></li>
    </ol>
	<div id="image-frame" class="carousel-inner">
		<div class="item active">
			<%= image_tag("la/la1.jpg", alt: "First image") %>
		</div>
		<div class="item">
			<%= image_tag("la/la2.jpg", alt: "Second image") %>
		</div>
		<div class="item">
			<%= image_tag("la/la3.jpg", alt: "Third image") %>
		</div>
	</div>
	<a class="left carousel-control" href='' data-slide="prev">
		<span class="glyphicon glyphicon-chevron-left"></span>
	</a>
	<a class="right carousel-control" href='' data-slide="next">
		<span class="glyphicon glyphicon-chevron-right"></span>
	</a>
</div>
<div class="photo-caption">
	<small><p><b>September 2013</b> | Tags: LA, b&amp;w, 35mm, yashica t4, film, photography, neopan, LAX, palmtree</p></small>
</div>
<%= render 'layouts/copyright' %>